<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <!-- JSPC servlet mappings start -->

    <servlet>
        <servlet-name>org.apache.jsp.SuccessSubscription_jsp</servlet-name>
        <servlet-class>org.apache.jsp.SuccessSubscription_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.UserLogin_jsp</servlet-name>
        <servlet-class>org.apache.jsp.UserLogin_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.WelcomeHomePage_jsp</servlet-name>
        <servlet-class>org.apache.jsp.WelcomeHomePage_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.ViewAvailableBooksUnSubscribed_jsp</servlet-name>
        <servlet-class>org.apache.jsp.ViewAvailableBooksUnSubscribed_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.UserRegistration_jsp</servlet-name>
        <servlet-class>org.apache.jsp.UserRegistration_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.ViewAvailableBooksSubscribed_jsp</servlet-name>
        <servlet-class>org.apache.jsp.ViewAvailableBooksSubscribed_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.UploadBookForm_jsp</servlet-name>
        <servlet-class>org.apache.jsp.UploadBookForm_jsp</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>org.apache.jsp.PaymentGateway_jsp</servlet-name>
        <servlet-class>org.apache.jsp.PaymentGateway_jsp</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.SuccessSubscription_jsp</servlet-name>
        <url-pattern>/SuccessSubscription.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.UserLogin_jsp</servlet-name>
        <url-pattern>/UserLogin.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.WelcomeHomePage_jsp</servlet-name>
        <url-pattern>/WelcomeHomePage.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.ViewAvailableBooksUnSubscribed_jsp</servlet-name>
        <url-pattern>/ViewAvailableBooksUnSubscribed.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.UserRegistration_jsp</servlet-name>
        <url-pattern>/UserRegistration.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.ViewAvailableBooksSubscribed_jsp</servlet-name>
        <url-pattern>/ViewAvailableBooksSubscribed.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.UploadBookForm_jsp</servlet-name>
        <url-pattern>/UploadBookForm.jsp</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>org.apache.jsp.PaymentGateway_jsp</servlet-name>
        <url-pattern>/PaymentGateway.jsp</url-pattern>
    </servlet-mapping>

<!-- JSPC servlet mappings end -->

<welcome-file-list>
    <welcome-file>UserRegistration.jsp</welcome-file>
  </welcome-file-list>
</web-app>
